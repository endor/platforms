{{#category_name}}
  <div id="category_header">
    <h2>{{category_name}}</h2>
    <a href="#/">back</a>
  </div>
{{/category_name}}

<div id="categories_container">
  <h2>Categories</h2>
  <ul id="categories">
    {{#categories}}
      <li>
        <a href="#/categories/{{esc_details}}">{{name}}</a>
      </li>
    {{/categories}}
  </ul>  
</div>

<div id="conferences_container">
  <h2>Conferences</h2>
  <ul id="conferences">
    {{#conferences}}
      <li>
        <a href="#/conferences/{{esc_details}}">{{name}}</a>
      </li>
    {{/conferences}}
  </ul>  
</div>

<br class="clear"/>